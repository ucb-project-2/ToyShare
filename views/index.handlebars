<div class="jumbotron jumbotron-fluid parallax" id="header">
  <div class="toplogo">
      <a href="index.html">
          <img src="img/logo.png" alt="logo">
      </a>
    <h3 class="welcome_header"><div id="welcome_text">Welcome to BabyBoo</div></h3><br>
  </div>
</div>
  <!-- Logo + menu start -->

<!--Example post (should be put into a partial later)  -->
<div class="container">
<ul>
  {{#each post}}
  	<li id="post_list">
       <p>Item: {{this.item_name}}<br>
         Poster: {{this.poster_name}}<br>
         Description: {{this.item_desc}}<br>
         Borrowed: {{this.borrowed}}</p>
  	</li>
  {{/each}}
</ul>
</div>
